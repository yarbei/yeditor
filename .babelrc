{
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": false
			}
		],
		["@babel/preset-react"]
	],
	"env": {
		"development": {
			"presets": [["@babel/preset-react", { "development": true }]]
		}
	},
	"plugins": [
		["@babel/plugin-transform-runtime"]
	],
	"ignore": [
		"node_modules/**"
	]
}